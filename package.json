{
  "name": "timelines",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.6.2",
    "cypress-testing-library": "2.2.0",
    "qs": "^6.5.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-notifications": "^1.4.3",
    "react-router-dom": "^4.3.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "cypress": "^3.0.1",
    "dom-testing-library": "^2.6.0",
    "faker": "^4.1.0",
    "react-scripts": "2.0.0-next.3e165448",
    "react-testing-library": "^3.1.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "npm run -s test:jest && npm run -s test:cypress",
    "test:jest": "react-scripts test --env=jsdom",
    "test:cypress": "cypress run --config fileServerFolder=build",
    "test:cypress:open": "cypress open",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/src/api/*.{js}",
      "!<rootDir>/src/registerServiceWorker.js",
      "!<rootDir>/src/setupTests.js"
    ]
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}
